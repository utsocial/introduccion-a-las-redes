    <!--answers-->
    <component id="ID_s1_ans01">
        <text>
            <![CDATA[Para IPv4, siempre está establecido en 0100.]]>
        </text>
    </component>
    <component id="ID_s1_ans02">
        <text>
            <![CDATA[Identifica la prioridad de cada paquete.]]>
        </text>
    </component>
    <component id="ID_s1_ans03">
        <text>
            <![CDATA[Comúnmente se conoce como "conteo de saltos".]]>
        </text>
    </component>
    <component id="ID_s1_ans04">
        <text>
            <![CDATA[Identifica el protocolo de capa superior que se utilizará a continuación.]]>
        </text>
    </component>
    <component id="ID_s1_ans05">
        <text>
            <![CDATA[Identifica la dirección IP del host emisor.]]>
        </text>
    </component>
    <component id="ID_s1_ans06">
        <text>
            <![CDATA[Identifica la dirección IP del host destinatario.]]>
        </text>
    </component>
